suite test_suite :
test test_fT_ME :
    overwrite : pf , cmake 
    srcpath : 'tdd-dsl/input/tdd_dev/UVic_example_opem'
    modules :
        cfo_example
        additional_use_0
        additional_use_1
    var :
        dp : integer, PARAMETER = KIND( 0D0 )
        xIntercept : real( dp ) = 1D0
        yIntercept : real( dp ) = - ( 283D0 / 520D0 )
    assert Equal :
        in :
            fT_ME( xIntercept ) , K # xIntercept  as input 
        out :
            -0.50769230769230766 , K # output 0 Kelvin
        tolerance : 1D-12
        failmessage : 'fails x-Intercept at 14.89D0'
        # asserts x-Intercept
    assert Equal :
        in :
            fT_ME( 0D0 ) , K # input 0 Kelvin
        out :
            yIntercept , K # test yIntercept as output
        tolerance : 1D-12
        failmessage : 'fails y-Intercept at -54.42D-2'
        # asserts y-Intercept
